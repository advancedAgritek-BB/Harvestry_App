{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Telemetry": {
    "WalReplication": {
      "Enabled": false
    },
    "Mqtt": {
      "Enabled": false,
      "Host": "localhost",
      "Port": 1883,
      "TopicFilter": "site/+/equipment/+/telemetry/#"
    },
    "Subscriptions": {
      "SnapshotIntervalSeconds": 10
    }
  }
}


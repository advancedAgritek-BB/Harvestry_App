{
  "Environment": "Development",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Port=5432;Database=harvestry_dev;Username=harvestry_user;Password=harvestry_dev_password;Include Error Detail=true",
    "Redis": "localhost:6379,abortConnect=false"
  },
  "Observability": {
    "ServiceName": "harvestry-api",
    "Environment": "development",
    "OpenTelemetry": {
      "Endpoint": "http://localhost:4318",
      "Protocol": "grpc"
    },
    "Sentry": {
      "Dsn": "",
      "Environment": "development",
      "TracesSampleRate": 1.0,
      "Debug": true
    }
  },
  "FeatureFlags": {
    "Unleash": {
      "UnleashApi": "http://localhost:4242/api",
      "ApiToken": "*:*.unleash-insecure-api-token",
      "AppName": "harvestry-api",
      "Environment": "development",
      "RefreshInterval": 15
    }
  },
  "Security": {
    "RLS": {
      "Enabled": true,
      "EnforceUserContext": true
    },
    "JWT": {
      "Secret": "dev-secret-key-do-not-use-in-production-replace-with-kms",
      "Issuer": "harvestry-dev",
      "Audience": "harvestry-api",
      "ExpirationMinutes": 60
    },
    "CORS": {
      "AllowedOrigins": [
        "https://localhost:3000",
        "http://localhost:3000"
      ],
      "AllowCredentials": true
    }
  },
  "Integrations": {
    "Slack": {
      "WebhookUrl": "",
      "BotToken": "",
      "SigningSecret": ""
    },
    "METRC": {
      "BaseUrl": "https://api-ca.metrc.com",
      "VendorApiKey": "",
      "UserApiKey": ""
    },
    "QuickBooks": {
      "ClientId": "",
      "ClientSecret": "",
      "Environment": "sandbox"
    }
  },
  "RateLimiting": {
    "Enabled": false,
    "PermitLimit": 100,
    "QueueLimit": 10,
    "Window": "00:01:00"
  }
}
